{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-console": false,
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-unused-expression": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": []
    }
}
